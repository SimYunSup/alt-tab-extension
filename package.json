{
  "name": "alt-tab-extension",
  "description": "Detects when tabs have been inactive for a specified period and automatically stores them",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:dev:firefox": "wxt build -b firefox --mode development",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "wxt": "wxt",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0",
